<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.start.webBack.mapper.RoleactionMapper"> 
	
	<!-- 根据cpId查询cpmId -->	
	<select id = "selectRoleaction" parameterType = "String" resultType = "String">
		SELECT
				ROLEACTION
		FROM
				ROLEACTIONINFO
		WHERE
				ROLEID = #{roleloginid}
	</select>
</mapper>